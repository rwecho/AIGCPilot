# crawler runtime env (separate from Next.js root .env)

# API endpoint of your Next.js tools API
CRAWLER_API_URL="http://localhost:3000/api/tools"

# Must match API_SECRET_KEY in Next.js root .env
API_SECRET_KEY="your-secret-key-for-crawler-auth"

# DeepSeek key for llm_processor.py
DEEPSEEK_API_KEY=""

# Cloudflare R2 (S3-compatible)
R2_ENDPOINT_URL="https://<accountid>.r2.cloudflarestorage.com"
R2_ACCESS_KEY_ID=""
R2_SECRET_ACCESS_KEY=""
R2_BUCKET_NAME=""
R2_REGION="auto"

# Public CDN/base URL for uploaded media, e.g. https://cdn.example.com
R2_PUBLIC_URL=""
